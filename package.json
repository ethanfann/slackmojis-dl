{
	"name": "slackmojis-dl",
	"version": "3.0.0",
	"description": "Downloads all emojis from https://slackmojis.com",
	"type": "module",
	"main": "dist/cli.js",
	"author": "Ethan Fann",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ethanfann/slackmojis-dl.git"
	},
	"bugs": {
		"url": "https://github.com/ethanfann/slackmojis-dl/issues"
	},
	"scripts": {
		"build": "tsc",
		"start": "tsx src/cli.tsx",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"update:metadata": "tsx scripts/update-slackmojis-metadata.ts"
	},
	"files": [
		"dist",
		"data"
	],
	"bin": {
		"slackmojis-dl": "dist/cli.js"
	},
	"dependencies": {
		"commander": "^12.1.0",
		"glob": "^7.2.0",
		"ink": "^6.3.1",
		"react": "^19.2.0",
		"undici": "^7.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/glob": "^8.1.0",
		"@types/node": "^20.14.9",
		"@types/react": "^19.0.0",
		"tsx": "^4.19.0",
		"typescript": "^5.6.3",
		"vitest": "^4.0.3"
	},
	"engines": {
		"node": ">=20"
	}
}
