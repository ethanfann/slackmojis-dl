{
  "name": "slackmojis_downloader",
  "version": "2.0.0",
  "description": "Downloads all emojis from https://slackmojis.com",
  "main": "app.js",
  "author": "Ethan Fann",
  "license": "MIT",
  "bin": "cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node cli.js"
  },
  "files": [
    "cli.js",
    "ui.js"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.5.3",
    "import-jsx": "^4.0.0",
    "ink": "^3.0.5",
    "ink-spinner": "^4.0.1",
    "meow": "^7.1.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@ethancord/prettier-config": "^1.0.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "2.1.2"
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "prettier": "@ethancord/prettier-config"
}
