{
	"name": "slackmojis-dl",
	"version": "3.0.0",
	"description": "Downloads all emojis from https://slackmojis.com",
	"type": "module",
	"main": "dist/cli.js",
	"author": "Ethan Fann",
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"start": "npm run build --silent && node dist/cli.js",
		"test": "ava",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"update:last-page": "node scripts/update-last-page.js",
		"prepublishOnly": "npm run build"
	},
	"bin": {
		"slackmojis-dl": "dist/cli.js"
	},
	"dependencies": {
		"@inkjs/ui": "^2.0.0",
		"axios": "^0.21.1",
		"glob": "^7.2.0",
		"ink": "^6.3.1",
		"meow": "^7.1.1",
		"react": "^19.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/node": "^20.14.9",
		"@types/react": "^19.0.0",
		"ava": "^3.15.0",
		"nock": "^13.5.6",
		"typescript": "^5.6.3"
	},
	"engines": {
		"node": ">=20"
	}
}
